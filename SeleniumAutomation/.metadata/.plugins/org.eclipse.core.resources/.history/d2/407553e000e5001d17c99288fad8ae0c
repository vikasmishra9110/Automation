package nagp.selenium.pageobjects;

import java.util.List;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class ShippingPageObjects {

@FindBy(xpath="//*[@name='street[0]']")
public WebElement streetAddress;
@FindBy(xpath="//*[@name='city']")
public WebElement city;
@FindBy(xpath="//select[@name='region_id']")
public WebElement stateProvinceDropDown;
@FindBy(xpath="//*[@name='postcode']")
public WebElement postalCode;
@FindBy(xpath="//*[@name='country_id']")
public WebElement country;
@FindBy(xpath="//*[@name='telephone']")
public WebElement phoneNo;
@FindBy(xpath="//input[@value='flatrate_flatrate']")
public WebElement shippingMethod;
@FindBy(xpath="//span[text()='Next']")
public WebElement nextButton;
@FindBy(xpath="//button[text()='Place Order']")
public WebElement placeOrderButton;



}
